{
  "name": "crudapi",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:single": "nodemon --exec npm run start:dev",
    "dev:multi": "nodemon --exec npm run start:dev:multi",
    "start:dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/singleServer.ts",
    "start:dev:multi": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/multiServer.ts",
    "start:prod": "tsc && node dist/singleServer.js",
    "start:prod:multi": "tsc && node dist/multiServer.js",
    "test": "jest",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix./src",
    "ts": "npx tsc --noEmit"
  },
  "author": "evvrod",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@eslint/js": "^10.0.0",
    "@types/jest": "^29.5.13",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "uuid": "^10.0.0"
  }
}
